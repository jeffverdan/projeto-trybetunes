{
    "env": {
      "browser": true,
      "es2020": true
    },
    "extends": "trybe-frontend",
    "parser": "babel-eslint",
    "rules": {
      "react/jsx-max-depth" : 0
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    },
    "plugins": ["react"]
  }
  